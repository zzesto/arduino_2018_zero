
int LEDpin = 13;
int waitTimeOn = 900;
int waitTimeOff = 100;
String wm1 = "Pin State";
String wm2 = " : ";
String wm3;



void setup ()
{
  Serial.begin(9600);
pinMode(LEDpin, OUTPUT);
wm3=wm1+wm2;
}

  void loop ()
  {
    
    digitalWrite(13, HIGH);
   delay(waitTimeOn);
   Serial.print(wm3);
   Serial.println(HIGH);
    digitalWrite(13, LOW);
   delay(waitTimeOff); 
    Serial.print(wm3);
    Serial.println(LOW);
   
  // pinMode(LEDpin, INPUT_PULLUP);
    }

    
